<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 1.6.4//EN"
        "http://google-web-toolkit.googlecode.com/svn/tags/1.6.4/distro-source/core/src/gwt-module.dtd">

<module>

    <!-- Inherit the core Web Toolkit stuff.                  -->
    <inherits name='com.google.gwt.user.User'/>
    <inherits name='com.extjs.gxt.ui.GXT'/>
    <inherits name='com.extjs.gxt.themes.Themes'/>
    <inherits name="com.google.gwt.user.TextBox"/>

    <!--<source path="server"/>-->
    <source path="client"/>
    <!-- Specify the app entry point class.                   -->
    <entry-point class='ibeans.client.IBeansConsole2'/>

    <!-- Specify the app servlets.                   -->
    <servlet path='/PluginsService' class='ibeans.server.PluginsServiceImpl'/>
    <servlet path="/IBeansCentralService" class="ibeans.server.IBeansCentralServiceImpl"/>
    <servlet path="/ApplicationService" class="ibeans.server.ApplicationServiceImpl"/>



    <stylesheet src="IBeans.css"/>
    <stylesheet src="TCatServer.css"/>
    <stylesheet src="extra.css"/>
    <stylesheet src="gxt/css/gxt-all.css"/>
</module>